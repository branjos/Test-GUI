/*
 * Creates the frame for the app
 */

import java.awt.*;
import java.awt.event.*;
import javax.swing.*;
/**
 *
 * @author Brandon Smith
 */
public class frame extends JFrame {
    
    public frame(){
        init();
    }
    
    
    public void init(){
        
        setTitle("Teen Challenge Urinalysis Form  v1.0");
        setSize(600, 700);
        setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
        setVisible(true);
 
       
        RadioPanel panel = new RadioPanel();
        
        add(panel);
        pack();
    }
    
}
